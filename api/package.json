{
  "name": "agent-ui-api",
  "version": "1.0.0",
  "description": "Backend API for Agent UI",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "deploy": "./deploy.sh",
    "deploy:dev": "ENVIRONMENT=development ./deploy.sh",
    "deploy:staging": "ENVIRONMENT=staging ./deploy.sh",
    "deploy:prod": "ENVIRONMENT=production ./deploy.sh",
    "logs": "sam logs --stack-name agent-ui-api --tail",
    "destroy": "aws cloudformation delete-stack --stack-name agent-ui-api"
  },
  "keywords": [
    "agent",
    "api",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-bedrock-agent-runtime": "^3.700.0",
    "@aws-sdk/client-cloudformation": "^3.700.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.160",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
